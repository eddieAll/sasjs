{
  "name": "sasjs",
  "description": "JavaScript adapter for SAS",
  "scripts": {
    "build": "rimraf build && webpack && cp package.json ./build && cp README.md ./build",
    "package:lib": "npm run build && cd build && npm pack",
    "publish:lib": "npm run build && cd build && npm publish",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest",
    "cypress": "./node_modules/.bin/cypress open",
    "semantic-release": "semantic-release"
  },
  "release": {
    "plugins": [
      "@semantic-release/npm",
      {
        "pkgRoot": "./build"
      }
    ]
  },
  "keywords": [
    "sas",
    "viya"
  ],
  "author": "Allan Bowe <support@macropeople.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/macropeople/sasjs"
  },
  "license": "ISC",
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.1.2",
    "@semantic-release/npm": "^7.0.4",
    "@types/jest": "^25.1.2",
    "cp": "^0.2.0",
    "cypress": "^4.1.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.0.4",
    "ts-jest": "^25.2.0",
    "ts-loader": "^6.2.1",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  }
}
